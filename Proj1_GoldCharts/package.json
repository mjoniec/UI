{
  "name": "ngx-echarts-demo",
  "version": "6.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4202",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href ./",
    "build:lib": "ng build ngx-echarts",
    "build:lib:prod": "ng build ngx-echarts --prod",
    "test": "ng test",
    "test:lib": "ng test ngx-echarts",
    "lint": "ng lint",
    "lint:lib": "ng lint ngx-echarts",
    "e2e": "ng e2e",
    "clean": "rm -rf dist",
    "compodoc": "compodoc -p projects/ngx-echarts/tsconfig.lib.json -d dist/docs/api-doc --theme stripe",
    "serve:doc": "compodoc -p projects/ngx-echarts/tsconfig.lib.json -s",
    "build:doc": "run-s build:prod compodoc",
    "update:file": "cpr README.md dist/ngx-echarts/README.md -o",
    "release": "run-s clean dist build:doc",
    "demo": "run-s build:lib start",
    "dist": "run-s build:lib:prod update:file",
    "prettier": "prettier --config .prettierrc.js --write '{src,e2e,projects}/**/*.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.1",
    "@angular/common": "~10.2.1",
    "@angular/compiler": "~10.2.1",
    "@angular/core": "~10.2.1",
    "@angular/forms": "~10.2.1",
    "@angular/platform-browser": "~10.2.1",
    "@angular/platform-browser-dynamic": "~10.2.1",
    "@angular/router": "~10.2.1",
    "echarts": "^5.0.2",
    "echarts-gl": "^2.0.2",
    "ng-zorro-antd": "^10.1.0",
    "ngx-echarts": "^6.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.1",
    "@angular/language-service": "~10.2.1",
    "@compodoc/compodoc": "^1.1.11",
    "@types/echarts": "^4.9.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "cpr": "^3.0.1",
    "html-loader": "^1.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.1.2",
    "ngx-markdown": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "protractor": "~5.4.3",
    "raw-loader": "^4.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~4.0.5"
  }
}
